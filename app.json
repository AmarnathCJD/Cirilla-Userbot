{
    "name": "Cirilla Userbot",
    "description": "A lightning fast telegram personal userbot.",
    "logo": "https://te.legra.ph/file/cb37180e3aaa92dac6f40.jpg",
    "keywords": ["Telegram", "Userbot", "Fast", "Team RLX", "Telethon"],
    "repository": "https://te.legra.ph/file/cb37180e3aaa92dac6f40.jpg",
    "env": {
        "API_KEY": {"description": "App ID get from my.telegram.org", "value": ""},
        "API_HASH": {
            "description": "Api Hash get from my.telegram.org", 
            "value": ""
        },
        "STRING_SESSION": {"description": "Telegram user string session", "value": ""},
        "BOT_TOKEN": {
            "description": "bot token, get from @botfather.",
            "required": true,
            "value": ""
        },
        "MONGO_DB_URL": {
            "description": "Database url get from mongodb.com",
            "value": ""
        },
        "SPOTIFY_CLIENT_ID": {
            "description": "spotify account client ID, get from developer.spotify.com",
            "required": false
        }, 
        "SPOTIFY_CLIENT_SECRET": {
            "description": "spotify client secret, get from developer.spotify.com",
            "required": false
        },
        "REM_BG_API_KEY": {
            "description": "Need for remove bg module, get from remove.bg",
            "required": false
        },
        "ALIVE_PHOTO": {
             "description": "Alive PIC telegraph link.",
             "required": false
        },
        "OWNER_ID": {
             "description": "userbot owner ID",
             "required": true
        }
    },
    "buildpacks": [{"url": "heroku/python"}]
}
